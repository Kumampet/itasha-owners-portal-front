{
  "name": "itasha-owners-portal-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "check": "npm run prisma:generate && npm run type-check && npm run lint",
    "db:seed": "tsx prisma/seed.ts",
    "db:clear-sessions": "tsx scripts/clear-sessions.ts",
    "prisma:generate": "prisma generate",
    "prepare": "node -e \"if (!process.env.CI) { try { require('husky').install() } catch(e) {} }\""
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@aws-sdk/client-ses": "^3.939.0",
    "@prisma/adapter-mariadb": "^7.0.0",
    "@prisma/client": "^7.0.0",
    "bcryptjs": "^3.0.3",
    "mariadb": "^3.4.5",
    "mysql2": "^3.15.3",
    "next": "16.0.3",
    "next-auth": "^5.0.0-beta.30",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.3",
    "husky": "^9.1.7",
    "prisma": "^7.0.0",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
