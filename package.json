{
  "name": "itasha-owners-portal-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "tsx server.ts",
    "build": "next build",
    "start": "NODE_ENV=production tsx server.ts",
    "lint": "eslint . --max-warnings=2",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "check": "npm run prisma:generate && npm run type-check && npm run lint",
    "db:seed": "tsx prisma/seed.ts",
    "db:clear-sessions": "tsx scripts/clear-sessions.ts",
    "db:view-users": "tsx scripts/view-users.ts",
    "db:fix-user-account": "tsx scripts/fix-user-account.ts",
    "test:reminder-check": "tsx scripts/test-reminder-check.ts",
    "prisma:generate": "prisma generate",
    "lambda:build": "bash scripts/build-lambda.sh",
    "lambda:build:win": "powershell -ExecutionPolicy Bypass -File scripts/build-lambda.ps1",
    "lambda:deploy": "bash scripts/deploy-websocket.sh",
    "lambda:deploy:win": "powershell -ExecutionPolicy Bypass -File scripts/deploy-websocket.ps1",
    "sam:install": "bash scripts/install-sam-cli.sh",
    "websocket:deploy": "npm run lambda:build && sam build --template-file infrastructure/websocket-api.yaml && sam deploy",
    "websocket:deploy:win": "npm run lambda:build:win && powershell -ExecutionPolicy Bypass -File scripts/deploy-websocket.ps1"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@aws-sdk/client-lambda": "^3.967.0",
    "@aws-sdk/client-ses": "^3.939.0",
    "@prisma/adapter-mariadb": "^7.0.0",
    "@prisma/client": "^7.0.0",
    "@types/web-push": "^3.6.4",
    "bcryptjs": "^3.0.3",
    "mariadb": "^3.4.5",
    "mysql2": "^3.15.3",
    "next": "16.0.3",
    "next-auth": "^5.0.0-beta.30",
    "next-pwa": "^5.6.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.8.3",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.3",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "prisma": "^7.0.0",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}