    "lambda:prisma-layer:deploy:prod": "bash scripts/deploy-prisma-layer-prod.sh",
    "lambda:group-message-reminder:deploy:staging": "bash scripts/deploy-group-message-reminder-staging.sh",
    "lambda:group-message-reminder:deploy:prod": "bash scripts/deploy-group-message-reminder-prod.sh",
